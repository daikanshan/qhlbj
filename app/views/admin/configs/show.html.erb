<p id="notice"><%= notice %></p>

<p>
  <strong>名称:</strong>
  <%= @admin_config.name %>
</p>

<p>
  <strong>图片:</strong>
  <%= image_tag @admin_config.avatar_url(:small) if @admin_config.avatar %>
</p>

<p>
  <strong>状态:</strong>
  <%= @admin_config.status %>
</p>
<% if @admin_config.url %>
<p>
  <strong>Url:</strong>
  <%= @admin_config.url %>
</p>
<% end %>

<%= link_to '编辑', edit_admin_config_path(@admin_config) %> |
<%= link_to '返回', admin_configs_path %>
